<launch>
  <!--Include description and control launch files-->
  <include file="$(find iiwa_arm)/launch/robot_description.launch"/>
  <include file="$(find iiwa_arm)/launch/robot_control.launch"/>
  <!--include file="$(find iiwa_moveit)/launch/move_group.launch"/-->

  <!--Launch a gazebo world-->
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(find iiwa_arm)/worlds/cafe.world"/>
    <arg name="paused" value="false"/>
    <arg name="use_sim_time" value="true"/>
    <arg name="gui" value="true"/>
    <arg name="headless" value="false"/>
    <arg name="debug" value="false"/>
  </include>

  <!--spawn a robot in gazebo world-->
  <node name="urdf_spawner" pkg="gazebo_ros" type="spawn_model" respawn="false"
    output="screen" args="-urdf -param
    robot_description -x 0 -y 0 -z 0.2 -R 0 -P 0 -Y 0 -model iiwa"/>

  <!--rviz-->
  <!--node name="$(anon rviz)" pkg="rviz" type="rviz" respawn="false"
    output="screen"/-->

</launch>
